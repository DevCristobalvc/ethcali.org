{
  "buildCommand": "echo 'No build step needed for static site'",
  "outputDirectory": "./",
  "rewrites": [
    {
      "source": "/",
      "destination": "/ethcali.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/events",
      "destination": "/events.html"
    },
    {
      "source": "/venues",
      "destination": "/ethcalivenues.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

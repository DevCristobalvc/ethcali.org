{
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.csv",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/ethcali.html"
    },
    {
      "src": "/index.html",
      "dest": "/ethcali.html"
    },
    {
      "src": "/events",
      "dest": "/events.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/venues",
      "dest": "/ethcalivenues.html"
    }
  ]
}

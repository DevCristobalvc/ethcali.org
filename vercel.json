{
  "version": 2,
  "public": true,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/",
      "dest": "/ethcali.html"
    },
    {
      "src": "/index",
      "dest": "/ethcali.html"
    },
    {
      "src": "/home",
      "dest": "/ethcali.html"
    },
    {
      "src": "/events",
      "dest": "/events.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/venues",
      "dest": "/ethcalivenues.html"
    }
  ],
  "headers": [
    {
      "source": "/team/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/branding/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

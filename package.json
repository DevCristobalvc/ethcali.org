{
    "name": "ethcaliorg",
    "version": "1.0.0",
    "description": "Ethereum Cali Community Website",
    "scripts": {
        "start": "npx http-server . -p 8000 -c-1",
        "dev": "npx live-server --port=8000 --no-browser",
        "build": "mkdir -p public && cp -r *.html *.css *.js *.csv team branding 'Sarun Pro' public/",
        "clean": "rm -rf public",
        "rebuild": "npm run clean && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wmb81321/ethcaliorg.git"
    },
    "keywords": [
        "ethereum",
        "cali",
        "community",
        "web3"
    ],
    "author": "William Martinez",
    "license": "ISC",
    "devDependencies": {
        "http-server": "^14.1.1",
        "live-server": "^1.2.2"
    }
} 